<div class="row-fluid">
	<div class="span3">
		<%= render partial: 'project' %>
	</div>
	<div class="span9">
		<%= render partial: 'shared/navtabs', :locals => {:tabcontent => 'Project', :project_id => @project.id, :activetab => 'Project Info'} %>
				<div class="tab-pane active" id="projectinfo">
					<div class="admincontainer">
						<p><%= @project.description %></p>
					</div>
				</div>
				<div class="tab-pane" id="projectimages">
					<%= render_gallery_for @project.images_by_ordinal, item_class: 'span2' if @project.has_images? %>
				</div>
		<%= render partial: 'shared/tabfooter' %>
	</div>
</div>
<div class="row-fluid">
	<% @posts.each do |post| %>
		<%= render partial: 'posts/post', :locals => {:post => post} %>
	<% end %>
</div>
