<%= form_for(@post) do |f| %>
	<%= f.label "Name" %>
	<%= f.text_field :name %>
	<%= f.label "Content" %>
	<%= f.text_area :content %>
	<%= f.label "Associated Project" %>
	<%= f.select :project, @projects %>
	<%= f.label "Post Title" %>
	<%= f.text_field :title %>
	<%= f.label "Category" %>
	<%= f.select :category, @categories %><br />
	<%= f.submit :submit, :class => 'btn' %>
<% end %>